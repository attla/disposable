{
    "name": "attla/disposable",
    "description": "A PHP Laravel to validate disposable emails.",
    "keywords": [
        "attla",
        "laravel",
        "disposable",
        "email",
        "checker"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Lucas Nicolau",
            "email": "lucas@octha.com"
        }
    ],
    "homepage": "https://github.com/attla/disposable",
    "require": {
        "php": "^7.2|^8.0",
        "guzzlehttp/guzzle": "^6.3|^7.0",
        "illuminate/support": "^5.5|^6.0|^7.0|^8.0|^9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0|^9.0",
        "symfony/phpunit-bridge": "^5.1"
    },
    "autoload": {
        "psr-4": {
            "Attla\\Disposable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Attla\\Disposable\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Attla\\Disposable\\ServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
